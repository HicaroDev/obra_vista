# ==================== DATABASE ====================
# Supabase PostgreSQL Connection String
# Formato: postgresql://postgres.[PROJECT_REF]:[PASSWORD]@aws-0-[REGION].pooler.supabase.com:6543/postgres
DATABASE_URL="postgresql://user:password@localhost:5432/obra_vista"

# ==================== JWT ====================
# Chave secreta para assinar tokens JWT (use uma string aleatória e segura)
JWT_SECRET="sua_chave_secreta_super_segura_aqui_mude_isso"
JWT_EXPIRES_IN="7d"

# ==================== SERVER ====================
PORT=3001
NODE_ENV="development"

# ==================== CORS ====================
# URL do frontend para permitir requisições
FRONTEND_URL="http://localhost:5173"

# ==================== SUPABASE (Opcional) ====================
# Caso queira usar recursos adicionais do Supabase
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_ANON_KEY="your-anon-key"
